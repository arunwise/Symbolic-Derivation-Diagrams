values(flip, [a,b]).
set_sw(flip, [0.5, 0.5]).

genlist(0, []).
genlist(N, L) :-
    N > 0,
    msw(flip, N, X),
    L = [X | L1],
    N1 is N-1,
    genlist(N1, L1).

palindrome([]).
palindrome([_X]).
palindrome([H|R]) :-
    R \= [],
    reverse(R, [T|R1]),
    {H=T},
    palindrome(R1).
    

query :-
    genlist(6, L),
    palindrome(L).

%% copied from lists package
reverse([], []).
reverse([H|T], L) :-
	reverse1(T, L, [H]).

reverse1([], L, L).
reverse1([H|T], L, L1) :-
	reverse1(T, L, [H|L1]).
