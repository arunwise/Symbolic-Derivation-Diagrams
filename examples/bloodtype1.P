values(gene,[a,b,o]).
set_sw(gene, [0.5, 0.25, 0.25]).

:- export(bloodtype(P)).

bloodtype(P) :- genotype(X,Y), {X=Y}, {P=X}.
bloodtype(P) :- genotype(X,Y), {X=o}, {P=Y}.
bloodtype(P) :- genotype(X,Y), {Y=o}, {P=X}.
bloodtype(P) :- genotype(X,Y), {X=a}, {Y=b}, P=ab.
bloodtype(P) :- genotype(X,Y), {X=b}, {Y=a}, P=ab.

:- export(genotype(X,Y)).
genotype(X,Y) :- msw(gene, 1, X), msw(gene, 2, Y).