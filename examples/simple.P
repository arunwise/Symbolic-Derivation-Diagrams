values(alpha, [a, b, c]).
values(num, [1, 2, 3]).

:- export(test1(X)).
test1(X) :- msw(alpha, I, X).

:- export(test2(X)).
test2(X) :- msw(alpha, I, X), {X = a}.

% should fail
:- export(test3(X)).
test3(X) :- msw(alpha, I, X), {X = 1}.

:- export(test4(X, Y)).
test4(X, Y) :- msw(alpha, I, X), msw(alpha, J, Y), {X=Y}.

:- export(test5(X, Y)).
test5(X, Y) :- msw(alpha, I, X), msw(alpha, J, Y), {X\=Y}.

% should fail
:- export(test6(X, Y)).
test6(X, Y) :- msw(alpha, I, X), msw(num, I, Y), {X=Y}.

% should fail
:- export(test7(X, Y)).
test7(X, Y) :- msw(alpha, I, X), msw(num, I, Y), {X\=Y}.

:- export(test8(X, Y)).
test8(X, Y) :- msw(alpha, I, X), X=Y.

:- export(test9).
test9 :- msw(alpha, I, X).
test9 :- msw(num, I, X).

% should fail
:- export(test10(X, Y)).
test10(X, Y) :- msw(alpha, I, X), msw(alpha, J, Y), {X=Y}, {X\=Y}.
